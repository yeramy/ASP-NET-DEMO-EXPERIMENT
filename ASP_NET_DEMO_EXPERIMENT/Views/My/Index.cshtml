@{
}
<h1>Vocab List</h1>
<p>Vocabulary list of mine</p>
<table>
    <tr>
        <th>Word</th>
        <th>Defintion</th>
    </tr>
    @foreach(var vocab in Model)
    {
        <tr>
            <td>@vocab.Word</td>
            <td>@vocab.Definition</td>
        </tr>
    }
</table>
    